<div id="article-show">

  <h1><%= @article.title %></h1>
  <%= link_to 'Edit', edit_article_path(@article) %> |
  <%= link_to 'Back', articles_path %>
  <p class="article-content"><%= @article.content%></p>
  <p class="text-right date-article-show"><%= @article.created_at.strftime("%d %B %Y") %></p>
</div>

